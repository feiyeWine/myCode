package cn.ThreadSyn;
/**
 * 单利设计模式：确保一个类只有一个对象
 * @author YR
 *
 */
public class JvmDemo01 {
    public static void main(String[] args) {
	
    }
}
class JvmThread extends  Thread{
   @Override
   public void run() {
    System.out.println(Thread.currentThread().getName()+"创建对象");
   }
}
class Jvm{
    private static Jvm intance = null;
    public  static Jvm getIntance (){
	if(null==intance){
	    intance = new Jvm();
	}
	return intance;
    }
}
